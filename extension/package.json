{
  "name": "jupyterlite-zest-bridge",
  "version": "1.0.0",
  "description": "JupyterLab extension that bridges JupyterLite with the Zest LTI platform via postMessage",
  "license": "MIT",
  "author": "Virtual Arkansas",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "files": [
    "lib/**/*.{d.ts,js,js.map}",
    "style/**/*.css",
    "jupyterlite_zest_bridge/labextension/**/*"
  ],
  "jupyterlab": {
    "extension": true,
    "outputDir": "jupyterlite_zest_bridge/labextension"
  },
  "scripts": {
    "build": "jlpm run build:ts && jlpm run build:labextension",
    "build:ts": "tsc",
    "build:labextension": "jupyter labextension build .",
    "clean": "rm -rf lib jupyterlite_zest_bridge/labextension",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^4.0.0",
    "@jupyterlab/notebook": "^4.0.0",
    "@jupyterlab/docmanager": "^4.0.0",
    "@jupyterlab/services": "^7.0.0",
    "@jupyterlab/apputils": "^4.0.0"
  },
  "devDependencies": {
    "typescript": "~5.3.0",
    "@jupyterlab/builder": "^4.0.0"
  }
}
